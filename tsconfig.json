{
    "compilerOptions": {
        "target": "es5",
        "module": "CommonJS",
        "lib": ["dom", "es2021", "ScriptHost"],
        "jsx": "react-jsx",
        "allowJs": true,
        "downlevelIteration": true,
        "esModuleInterop": true,
        "preserveSymlinks": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "useUnknownInCatchVariables": false
    },
    "include": ["src"]
}